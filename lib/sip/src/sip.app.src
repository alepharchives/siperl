%% This is the application resource file (.app file) for the sip,
%% application.
{application, sip,
  [{description, "SIP support"},
   {vsn, "0.1.0"},
   {registered,[sip_sup]},
   {applications, [kernel, stdlib, gproc]},
   {mod, {sip_app,[]}},
   {start_phases, []},
   {env, [
		  {udp, [5060]},
		  {tcp, [5060]},
		  {t1, 500},
		  {t2, 4000},
		  {t4, 5000}
		 ]}]}.
